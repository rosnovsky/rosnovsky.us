{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/fonts/inter-var-latin.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/podtema-com",
      "destination": "/about"
    }
  ],
  "rewrites": [
    { "source": "/blog/:year/:month/:day/:slug", "destination": "/blog/:slug" }]
}
