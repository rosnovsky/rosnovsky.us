{
  "name": "rosnovsky.us",
  "version": "6.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "deploy": "turbo run deploy",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.8.4",
    "turbo": "^1.6.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@7.13.2"
}
