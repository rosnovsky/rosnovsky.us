{
  "root": true,
  "project": {
    "name": "Rosnovsky Park Blog",
    "basePath": "/studio"
  },
  "api": {
    "projectId": "n3o7a5dl",
    "dataset": "prod"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "@sanity/code-input",
    "asset-source-unsplash",
    "mapbox-input",
    "vercel-deploy",
    "@sanity/dashboard",
    "media-library",
    "icon-picker",
    "@sanity/vision",
    "@andre-brdoch/sanity-plugin-schema-inspector"
  ],
  "env": {
    "development": {
      "plugins": [
        "@sanity/vision"
      ]
    }
  },
  "parts": [
    {
      "name": "part:@sanity/base/schema",
      "path": "./schemas/schema"
    },
    {
      "implements": "part:@sanity/form-builder/input/image/asset-sources",
      "path": "./parts/assetSources.ts"
    },
    {
      "implements": "part:@sanity/form-builder/input/file/asset-sources",
      "path": "./parts/assetSources.ts"
    }
  ]
}
